{
    "version": 2,
    "name": "Blinkit-clone",
    "builds": [
        {
            "src": "./server/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/src/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}